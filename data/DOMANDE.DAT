

(domanda  (attributo anni-dispositivo)
(testo-domanda "Quanti anni ha il dispositivo?")
          (risposte-valide 0-3-anni 4-6-anni 7-anni sconosciuto)
          (descrizione-risposte "Fino a tre anni" "Da quattro a sei anni" "Piu' di sei anni" "Non so")
          (spiegazione-risposte "Il dispositivo ha fino a tre anni di eta'" "Il dispositivo ha tra i quattro e sei anni di eta'" "Il dispositivo ha piu' di sei anni di eta'" "L'utente non sa quanti anni ha il dispositivo")
(spiegazione "Questa domanda permette di rendere piu' precisa la ricerca di diagnosi e soluzioni sulla base di quanto il dispositivo potrebbe soffrire di determinati
problemi legati all'eta'.")
(help "Rispondere con l'eta' (o una stima dell'eta') del dispositivo. ")
)

(domanda  (attributo garanzia)
(testo-domanda "Il dispositivo e' ancora in garanzia?")
          (risposte-valide si no sconosciuto)
          (descrizione-risposte "Si" "No" "Non so")
          (spiegazione-risposte "Il dispositivo e' coperto da garanzia" "Il dispositivo non e' coperto da garanzia" "Non si sa se il dispositivo e' coperto da garanzia")
(spiegazione "Questa domanda aiuta il sistema a trovare soluzioni utili per risolvere il problema. ")
(help "Verificate se il dispositivo e' ancora coperto da garanzia.")
)

(domanda  (attributo tipo-dispositivo)
(testo-domanda "A quale tipologia appartiene il dispositivo?")
          (risposte-valide pc-desktop pc-portatile)
          (descrizione-risposte "PC Desktop" "PC Portatile/Netbook")
          (spiegazione-risposte "Il dispositivo e' un PC desktop" "Il dispositivo e' un portatile o netbook")
(spiegazione "Alcune diagnosi, soluzioni e domande possono variare sensibilmente in base alla tipologia del dispositivo. ")
(help "Rispondere in base alla tipologia del dispositivo.")
)

(domanda  (attributo utente-proprietario)
(testo-domanda "Il dispositivo e' di proprieta' dell'utente?")
          (risposte-valide si no)
          (descrizione-risposte "Si" "No")
          (spiegazione-risposte "Il dispositivo e' di proprieta' dell'utente" "Il dispositivo non e' di proprieta' dell'utente")
(spiegazione "Rispondendo Si a questa domanda permette al sistema di porre domande aggiuntive all'utente per rendere la diagnosi piu' precisa.")
(help "Rispondere Si se l'utente e' il proprietario o l'utilizzatore principale del dispositivo.")
)

(domanda  (attributo esperienza-utente)
(testo-domanda "Indicare il grado di esperienza dell'utente.")
          (risposte-valide utente-esperto utente-inesperto)
          (descrizione-risposte "Sono un utente esperto con competenze informatiche e conoscenza dei termini tecnici utilizzati"
                                "Dispongo di competenze informatiche limitate o non conosco i termini tecnici utilizzati")
          (spiegazione-risposte "L'utente e' un esperto dotato di competenze informatiche" "L'utente dispone di competenze informatiche limitate")
(spiegazione "Questa domanda viene posta per indicare al sistema quali domande porre all'utente, in modo che risultino piu' comprensibili se l'utente e' inesperto.")
(help "Si consiglia di rispondere come 'utente esperto' se si ha un minimo di esperienza sulle componenti del dispositivo.")
)





(domanda  (attributo ha-batteria)
(testo-domanda "Il dispositivo ha la batteria di alimentazione inserita?
ATTENZIONE: Per il corretto svolgimento dell'analisi e' necessario che il dispositivo sia connesso ad una presa elettrica.")
          (risposte-valide si no)
          (descrizione-risposte "Si, la batteria e' inserita" "No, il dispositivo non ha una batteria oppure non e' inserita")
          (spiegazione-risposte "Il dispositivo ha la batteria inserita" "Il dispositivo non ha una batteria oppure non e' inserita")
(spiegazione "Questa domanda permette al sistema di identificare possibili problemi relativi all'alimentazione della batteria.")
(help "Solitamente la batteria e' locata nella parte inferiore ed e' necessario capovolgere il dispositivo per accedervi. Per sganciarla potrebbe essere
necessario premere uno o piu' tasti. In caso di dubbi si consiglia di consultare il manuale del dispositivo.")
)


(domanda  (attributo domanda-accensione)
